{
    "name": "solana-toolkit",
    "version": "0.0.2",
    "description": "Utilities for Solana developers",
    "author": "NguyenNguyen <nguyenbk92@gmail.com>",
    "repository": "https://github.com/thanhnguyennguyen/solana-toolkit",
    "license": "MIT",
    "keywords": [
        "typescript",
        "solana",
        "toolkit"
    ],
    "main": "./dist/tsc/main.js",
    "types": "./dist/tsc/main.d.ts",
    "browser": "./dist/esbuild/browser.js",
    "bin": {
        "my-cli-tool": "./dist/esbuild/cli.js"
    },
    "scripts": {
        "cli": "ts-node src/cli.ts",
        "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
        "lint-fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
        "test": "jest",
        "clean": "rm -rf dist build package",
        "ts-node": "ts-node",
        "docs": "typedoc --entryPoints src/main.ts",
        "build": "tsc -p tsconfig.json",
        "fresh-build": "yarn clean && yarn build"
    },
    "devDependencies": {
        "@types/jest": "^26.0.21",
        "@types/node": "^15.0.1",
        "@typescript-eslint/eslint-plugin": "^4.19.0",
        "@typescript-eslint/parser": "^4.19.0",
        "esbuild": "^0.11.11",
        "eslint": "^7.22.0",
        "jest": "^26.6.3",
        "ts-jest": "^26.5.4",
        "ts-node": "^9.1.1",
        "typedoc": "^0.20.35",
        "typescript": "^4.2.3"
    },
    "dependencies": {
        "@solana/web3.js": "^1.34.0",
        "@types/bs58": "^4.0.1",
        "axios": "^0.25.0",
        "bs58": "^4.0.1"
    }
}
